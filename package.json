{
  "name": "squad-planner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc -b",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "preview": "react-router-serve ./build/server/index.js",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:a11y": "vitest run --reporter=verbose src/components/ui/__tests__/a11y.test.tsx",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --config=lighthouserc-mobile.json",
    "analyze": "set ANALYZE=true&& react-router build",
    "analyze:unix": "ANALYZE=true react-router build",
    "optimize-images": "node scripts/optimize-images.mjs",
    "remotion:preview": "npx remotion studio src/remotion/index.ts",
    "remotion:render": "npx remotion render src/remotion/index.ts HeroVideo out/hero-video.mp4",
    "remotion:render:gif": "npx remotion render src/remotion/index.ts HeroVideo out/hero-video.gif --image-format=png"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@livekit/components-react": "^2.9.19",
    "@react-router/dev": "^7.13.0",
    "@react-router/node": "^7.13.0",
    "@react-router/serve": "^7.13.0",
    "@remotion/cli": "^4.0.421",
    "@remotion/google-fonts": "^4.0.421",
    "@remotion/player": "^4.0.421",
    "@remotion/transitions": "^4.0.421",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.94.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "@vercel/react-router": "^1.2.5",
    "@vitejs/plugin-rsc": "^0.5.19",
    "babel-plugin-react-compiler": "^1.0.0",
    "canvas-confetti": "^1.9.4",
    "framer-motion": "^12.31.0",
    "isbot": "^5.1.34",
    "livekit-client": "^2.17.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "remotion": "^4.0.421",
    "remotion-skills": "github:remotion-dev/skills",
    "sonner": "^2.0.7",
    "zod": "^3.22.3",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "axe-core": "^4.11.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
